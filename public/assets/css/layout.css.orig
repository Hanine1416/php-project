}

.main-menu .dropdown:hover .discipline-menu {
  display: block;
}
.discipline-menu-wrap {
  position: relative;
}
.discipline-menu-wrap .discipline-menu {
  position: absolute;
  padding: 0;
  margin: 0;
  display: none;
  z-index: 200;
  border: 1px solid #DCDCDC;
  background: #fff;
  width: 95%;
  top: 31px;
  margin: 0px 2.5%;
}
.discipline-menu-wrap .discipline-menu .discipline-content {
  overflow: hidden;
  height: inherit;
  height: 350px;
  margin: 40px 10px 20px 30px;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-menu {
  float: left;
  width: 30%;
  padding: 0 1%;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-menu a {
  margin: 0;
  text-transform: uppercase;
  font-weight: bold;
  color: #000;
  font-size: 0.85em;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-menu li:hover a,
.discipline-menu-wrap .discipline-menu .discipline-content .disc-menu .active a {
  border-bottom: 1px solid #000 !important;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display {
  float: left;
  overflow: auto;
  height: 350px;
  width: 70%;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu {
  display: none;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu.active {
  display: block;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu ul li {
  margin: 0;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu ul li.even {
  float: right;
  width: 50%;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu ul li.odd {
  float: left;
  width: 50%;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu ul li a {
  color: #000;
  font-size: 0.8em;
  line-height: 1;
  display: block;
  padding: 5px 0;
}
.discipline-menu-wrap .discipline-menu .discipline-content .disc-display .inner-menu ul li a:hover {
  text-decoration: underline;
}

