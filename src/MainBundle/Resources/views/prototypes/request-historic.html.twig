                        </div>
                    {% endif %}
                    {% if(statusHistoric == 'MixedRenewal') %}
                    {% endif %}
                    {% if(statusHistoric == 'Expired') %}
                        {% set expiredBook = book %}
                        {% set studentNumber = '' %}
                        {% if(book['Institutions']|length > 0) %}
                            {% if expiredBook['Institutions']['Institution'][0] is defined %}
                                {% set studentNumber = expiredBook['Institutions']['Institution'][0]['Students'] %}
