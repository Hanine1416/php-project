                                {% set endDate =  date(expiredBook.Details.publicationDate[lang])|date('Y') %}
                              {% else %}
                                  {% set endDate =  date(expiredBook.EndDate)|date('Y') %}
                            {% endif %}
                            <div class="date">{{ endDate }}</div>
                            <div class="books-history-container">
                                <div class="row book-content">
                                    <div class="columns large-8 small-12 medium-8 p-0 part-one">
                                        <div class="course-name">{{ "user.profile.books.expired"|trans }} </div>
                                        {% if studentNumber > 0 %}
                                            <div class="student-number">{{ 'historical_data.students_number'|trans }} : <span class="number">{{ studentNumber }}</span></div>
