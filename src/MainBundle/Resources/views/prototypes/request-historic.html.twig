                    {% if(statusHistoric == 'NotRenewed') %}
                        <div class="timeline-item">
                            {% set notRenewedYear =  date((myBooks['NotRenewed'][book.Isbn]['adoptions']|last)['EstimatedClose'])|date('Y') %}
                            <div class="date">{{ notRenewedYear }}</div>
                            <div class="books-history-container">
                                <div class="course-name">
                                    {{ 'user.profile.books.not_renewed'|trans }} {{ (notRenewedYear-1) ~'book.review.slash'|trans~notRenewedYear }}
